<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="RenameController"
            prefHeight="100.0" prefWidth="300.0">
    <VBox>
        <TextField fx:id="renameField"/>
        <HBox>
            <Button fx:id="renameButton" text="Rename" onMouseClicked="#rename"/>
            <Button fx:id="cancelButton" text="Cancel" onMouseClicked="#disappear"/>
        </HBox>
    </VBox>
</AnchorPane>
